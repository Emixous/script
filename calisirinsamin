-- Scripts:

local function RRXUDP_fake_script() -- basla.Script 
	local script = Instance.new('Script', basla)

	_G.bruh = true
	while wait() do
	    while _G.bruh == true do
	        wait()
	        pcall(function()
	            game.Players.LocalPlayer.Character:BreakJoints()
	        end)
	        game.ReplicatedStorage.money:FireServer()
	        game.ReplicatedStorage.respawn:FireServer()
	    end
	    pcall(function()
	        workspace.CurrentCamera:ClearAllChildren()
	        game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true);
	        game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom;
	        game.Players.LocalPlayer.PlayerGui.DEAD:Destroy()
	    end)
	    pcall(function()
	        game.ReplicatedStorage.money:FireServer()
	        game.ReplicatedStorage.respawn:FireServer()
	    end)
	end
end
coroutine.wrap(RRXUDP_fake_script)()
local function QOCKJ_fake_script() -- dur.Script 
	local script = Instance.new('Script', dur)

	_G.bruh = false
end
coroutine.wrap(QOCKJ_fake_script)()
local function BHGUP_fake_script() -- actim.LocalScript 
	local script = Instance.new('LocalScript', actim)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.emixinyarragi.Visible == false then
			script.Parent.Parent.emixinyarragi.Visible = true 
		else
			script.Parent.Parent.emixinyarragi.Visible = false
		end
	end)
end
coroutine.wrap(BHGUP_fake_script)()
